import random

print("Welcome to the Random password generator")

radomchar = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789!'*.?@|~"

noofpwd = int(input("Enter the number of passwords You want :"))

lenofpwd = int(input("Enter the Length of the password : "))

for x in range (noofpwd):
    pwd = ""
    for chars in range (lenofpwd):
        pwd = pwd + random.choice(radomchar)
    print(pwd)
